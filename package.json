    "tsc-watch": "6.2.0",
    "typescript": "5.4.5",
    "wait-on": "^7.2.0"
  },
  "resolutions": {
    "async": "^3.2.4",
    "es5-ext": "0.10.64",
    "node-forge": "^1.0.0",
    "set-value": "^4.0.1",
    "ansi-regex": "^5.0.1",
    "ssh2": "^1.4.0",
    "json-schema": "^0.4.0",
    "ip": "^2.0.1",
    "tar": "6.2.1",
    "minimatch": "^5.0.0",
    "semver": "^7.6.2",
    "tough-cookie": "4.1.4"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "yarn biome check --write --no-errors-on-unmatched"
    ],
    "*.{jsx,tsx}": [
      "yarn biome check --write --no-errors-on-unmatched"
    ],
    "*.json": [
      "yarn biome format --write --no-errors-on-unmatched"
    ]
  },
  "packageManager": "yarn@4.3.1"
}
