    "ip": "^2.0.1",
    "minimatch": "^5.0.0",
    "semver": "^7.5.3",
    "tough-cookie": "4.1.3"
  },
  "lint-staged": {
    "*.{js,ts}": ["biome check --apply"],
    "*.json": ["biome format --write --no-errors-on-unmatched"]
  }
}
